<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>PlacesToVisitInKarnataka</display-name>
  <welcome-file-list>
    <welcome-file>WelcomePage.html</welcome-file>
  </welcome-file-list>
  <servlet>
  <servlet-name>homePageServlet</servlet-name>
  <servlet-class>com.xworkz.karnataka.PlacesOfKarnataka</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>homePageServlet</servlet-name>
  <url-pattern>/homePageurl</url-pattern>
  </servlet-mapping>
  
  <servlet> 
  <servlet-name>registerationServlent</servlet-name>
  <servlet-class>com.xworkz.karnataka.Register</servlet-class>
  </servlet>
   <servlet-mapping>
  <servlet-name>registerationServlent</servlet-name>
  <url-pattern>/registerPage</url-pattern>
  </servlet-mapping>
  <servlet>
   <servlet-name>loginServlent</servlet-name>
  <servlet-class>com.xworkz.karnataka.LoginServlent</servlet-class>
  </servlet>
   <servlet-mapping>
  <servlet-name>loginServlent</servlet-name>
  <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>RegisterPage.html</welcome-file>
  </welcome-file-list>
</web-app>